public class Bilet
        {
            public int Id { get; set; }
            public string Name { get; set; }
            public bool gender { get; set; }
            public string email { get; set; }

            public otobus otobus { get; set; }

            public sefer sefer { get; set; }

          
            public Bilet(string Name, int Id, bool gender, string email, otobus otobus, sefer sefer)
            {
                this.Id = Id;
                this.Name = Name;
                this.gender = gender;
                this.email = email;
                this.otobus = otobus;
                this.sefer = sefer;
            }
        }
